{
  "name": "chaitanyagadodia.github.io",
  "version": "1.0.0",
  "description": "React, Next.js, Typescript, Eslint,Prettier, Emotion Portfolio Website",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build;",
    "start": "next start",
    "export": "next export",
    "deploy": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "lint": "eslint --fix './{components,pages,styles}/**/*.{ts,tsx}'"
  },
  "keywords": [],
  "author": "Chaitanya Gadodia <chaitanya.gadodia01@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "next": "^9.4.0",
    "next-pwa": "^2.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "imagemin-lint-staged": "^0.4.0",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "spritesmith-cli": "^1.1.2",
    "typescript": "^3.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  }
}
